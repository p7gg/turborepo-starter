{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"lint": {
			"outputs": [],
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"cache": true,
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		}
	}
}
